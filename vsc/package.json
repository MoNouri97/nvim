{
  "name": "my-vscode-snippets",
  "engines": {
    "vscode": "^1.11.0"
  },
  "contributes": {
    "snippets": [
      {
        "language": "cs",
        "path": "./snippets/csharp.json"
      },
      {
        "language": "lua",
        "path": "./snippets/lua.json"
      },
      {
        "language": [
          "gd",
          "gdscript"
        ],
        "path": "./snippets/gd.json"
      },
      {
        "language": [
          "typescriptreact",
          "tsx",
          "ts",
          "typescript"
        ],
        "path": "./snippets/tsx.json"
      }
    ]
  }
}
